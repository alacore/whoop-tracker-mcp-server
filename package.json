{
  "name": "whoop-mcp",
  "version": "1.0.0",
  "description": "MCP (Model Context Protocol) server for Whoop API integration",
  "main": "dist/mcp-standalone.js",
  "bin": {
    "whoop-mcp": "./dist/mcp-standalone.js"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22.0.0",
